{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "pages:build": "bun x @cloudflare/next-on-pages@beta",
    "pages:deploy": "bun x wrangler pages deploy .vercel/output/static",
    "pages:dev": "bun x wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "prettier": "prettier --check . --ignore-path .gitignore",
    "prettier:fix": "prettier --write . --ignore-path .gitignore",
    "start": "next start",
    "test": "echo \"No tests found\" && exit 0",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.0.10",
    "next": "^14.0.4",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-ixn-next": "latest",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vercel": "latest"
  }
}

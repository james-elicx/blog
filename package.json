    "next-themes": "0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "shiki": "1.20.0",
    "tailwind-merge": "1.14.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "20.4.4",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-ixn-next": "latest",
    "postcss": "8.4.47",
